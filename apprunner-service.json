{
  "ServiceName": "augentik-backend-service",
  "SourceConfiguration": {
    "ImageRepository": {
      "ImageIdentifier": "778185677217.dkr.ecr.us-east-1.amazonaws.com/augentik-backend:amd-final",
      "ImageConfiguration": {
        "Port": "8000",
        "RuntimeEnvironmentVariables": {
          "HOST": "0.0.0.0",
          "PORT": "8000",
          "CORS_ORIGINS": "https://augentik.com,https://api.augentik.com",
          "LLM_BINDING": "openai",
          "EMBEDDING_BINDING": "openai",
          "LLM_MODEL": "gpt-4o-mini",
          "EMBEDDING_MODEL": "text-embedding-3-large",
          "AUTH_ACCOUNTS": "amd.admin@augentik.com:SetStrongPass#2025",
          "TOKEN_SECRET": "amd-demo-jwt-secret-20250917",
          "JWT_ALGORITHM": "HS256",
          "WEBUI_TITLE": "Augentik - AMD Dashboard",
          "WEBUI_DESCRIPTION": "LightRAG Backend for AMD"
        }
      },
      "ImageRepositoryType": "ECR"
    },
    "AutoDeploymentsEnabled": false
  },
  "InstanceConfiguration": {
    "Cpu": "1 vCPU",
    "Memory": "2 GB",
    "InstanceRoleArn": "arn:aws:iam::778185677217:role/AugentikAmdTaskRole"
  },
  "HealthCheckConfiguration": {
    "Protocol": "HTTP",
    "Path": "/health",
    "Interval": 10,
    "Timeout": 5,
    "HealthyThreshold": 1,
    "UnhealthyThreshold": 5
  }
}